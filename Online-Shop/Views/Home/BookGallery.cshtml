@model List<Book> 
@{
    //ViewData["Title"] = "BookGallery";
}

<!doctype html>
<html>
<head>
    <title>Hello ASP.NET</title>
    <meta charset="utf-8" />
</head>
<body>
    <h2>Find the book for you</h2>
<table>
    <tr>
        <td>Name</td>
        <td>Author</td>
        <td>Price</td>
        <td></td>
    </tr>
    @foreach (var b in Model)
        { <tr>
            <td>@b.Name</td>
              <td>@b.Author</td>
              <td>@b.Price</td>
              </tr>
        }
</table>
</body>
</html>
